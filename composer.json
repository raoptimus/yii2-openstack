{
    "name": "raoptimus/yii2-openstack",
    "description": "Openstack/swift client for Yii2 Framework",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "yii 2",
        "yii2-extension",
        "yii2-openstack",
        "yii2 openstack",
        "yii2-swift",
        "yii2 swift",
        "php"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Evgeniy Urvantsev",
            "email": "resmus@gmail.com",
            "homepage": "https://github.com/raoptimus"
        }
    ],
    "support": {
        "issues": "https://github.com/raoptimus/yii2-openstack/issues",
        "source": "https://github.com/raoptimus/yii2-openstack"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "ext-fileinfo": "*",
        "yiisoft/yii2": "^2.0.14",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {
            "raoptimus\\openstack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "raoptimus\\openstack\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
